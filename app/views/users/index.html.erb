<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <ul>
      <% @users.each do |user| %>
        <li>
              <%= link_to user.first_name,user %><br>
                <%= link_to 'Show', user %>
                <%= link_to 'Edit', edit_user_path(user) %>
                <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %>
              
        </li>
       
    <% end %> 
  </ul>
  </tbody>
</table>
<br>
<%= link_to 'New User', new_user_path %>
